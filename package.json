{
  "name": "smapp-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --verbose=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pull-request-validator": "npm run lint && npm run test",
    "stub": "cd stub && nodemon index.ts",
    "mock": "ng serve --configuration=mock"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run pull-request-validator"
    }
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "./src/setupJest.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@types/hapi": "^18.0.3",
    "echarts": "^4.4.0",
    "eva-icons": "^1.1.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "ngx-echarts": "^4.2.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.2.0",
    "@angular-devkit/build-angular": "^0.803.19",
    "@angular/cli": "^8.3.19",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/echarts": "^4.4.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.147",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "hapi": "^18.1.0",
    "husky": "^3.0.9",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.9.0",
    "jest-preset-angular": "^8.0.0",
    "nodemon": "^1.19.4",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
